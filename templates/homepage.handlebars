<!DOCTYPE html>
<html lang="{{language}}">
{{> head }}
<body>
    {{> header}}
    <main>

        <div class="container">

            <h2>Posts:</h2>

            {{#each posts as |post|}}
                <article>

                    <h3>{{post.title}}</h3>
                </article>

        </div>
      
    </main>
</body>
</html>
