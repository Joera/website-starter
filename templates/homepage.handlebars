<!DOCTYPE html>
<html lang="{{language}}">
{{> head }}
<body>
    {{> header}}
    <main>

        <section class="container small_container">

            <h2>Posts:</h2>

            {{#each posts as |post|}}
                <article>
                    {{json this}}

                    <h3>{{post.title}}</h3>
                </article>
            {{/each}}

        </div>
      
    </main>
</body>
</html>
